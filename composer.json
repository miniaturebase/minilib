{
    "name": "minibase-app/minilib",
    "description": "A collection of random, quality of life functions for PHP â€“ a sort of kitchen-sink toolbox.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Brauer",
            "email": "18744334+jordanbrauer@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11",
        "friendsofphp/php-cs-fixer": "^2.18",
        "pestphp/pest": "^0.3.7",
        "phpstan/phpstan": "^0.12.58",
        "phpunit/phpunit": "9.3.11",
        "psy/psysh": "^0.10.4",
        "symfony/var-dumper": "~4"
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    },
    "autoload": {
        "psr-4": {
            "Minibase\\": "src/"
        },
        "files": [
            "src/append.php",
            "src/array_keys_exist.php",
            "src/array_make.php",
            "src/between.php",
            "src/bits.php",
            "src/blank.php",
            "src/camel_case.php",
            "src/clamp.php",
            "src/class_basename.php",
            "src/collapse.php",
            "src/exists.php",
            "src/filter.php",
            "src/generate.php",
            "src/head.php",
            "src/here.php",
            "src/ini.php",
            "src/ip.php",
            "src/is_associative.php",
            "src/is_console.php",
            "src/is_falsey.php",
            "src/is_truthy.php",
            "src/is_web.php",
            "src/is_windows.php",
            "src/is_unix.php",
            "src/kebab_case.php",
            "src/numeronym.php",
            "src/map.php",
            "src/me.php",
            "src/not.php",
            "src/only.php",
            "src/ordinal.php",
            "src/partial.php",
            "src/partition.php",
            "src/pascal_case.php",
            "src/path.php",
            "src/pluck.php",
            "src/prepend.php",
            "src/random_float.php",
            "src/read.php",
            "src/reduce.php",
            "src/retry.php",
            "src/runtime.php",
            "src/snake_case.php",
            "src/str_random.php",
            "src/swap.php",
            "src/tail.php",
            "src/tap.php",
            "src/transform.php",
            "src/value.php",
            "src/whoami.php",
            "src/with.php",
            "src/wrap.php",
            "src/write.php",
            "src/zip.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Minibase\\Tests\\": "tests/",
            "Minibase\\Tests\\Unit\\": "tests/Unit/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyze": "vendor/bin/phpstan analyse --configuration=phpstan.neon",
        "style": "vendor/bin/php-cs-fixer fix -vvv --config=.php_cs",
        "test": "vendor/bin/pest"
    }
}
